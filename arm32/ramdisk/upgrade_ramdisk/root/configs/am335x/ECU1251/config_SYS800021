cpu=am335x
product_name=sys800021
disk_type=mmc
partition_label=mbr
partition_num=4

# the first partition is boot
partition_boot=1

part_p1=40M fat32 boot
part_p2=350M ext3 rootfs
part_p3=200M ext3 recovery
part_p4=remaining ext3 data

rootfs_file=rootfs.tar.gz

partdevice=mmcblk0
bootdev=mmcblk0p1
rootdev=mmcblk0p2
recoverydev=mmcblk0p3
datadev=mmcblk0p4

external_update_dev=mmcblk1p1
internal_update_dev=mmcblk0p3

